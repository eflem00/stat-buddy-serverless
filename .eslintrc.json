{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "linebreak-style": 0,
    "no-await-in-loop": 0,
    "prefer-destructuring": 0,
    "quote-props": 0,
    "no-underscore-dangle": 0
  },
  "env": {
    "jest": true,
    "browser": false,
    "node": true,
    "es6": true
  }
}
