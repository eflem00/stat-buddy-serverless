{
  "name": "stat-buddy-serverless",
  "version": "1.0.0",
  "description": "Serverless Infrastructure for statbuddy.ca",
  "main": "handler.js",
  "scripts": {
    "local:games": "serverless invoke local --function games",
    "local:profiles": "serverless invoke local --function profiles",
    "local:api": "serverless offline start",
    "remote:games": "serverless invoke -f games -l",
    "remote:profiles": "serverless invoke -f profiles -l",
    "remote:api": "serverless invoke -f api -l",
    "deploy": "serverless deploy -v",
    "deploy:games": "serverless deploy function -f games",
    "deploy:profiles": "serverless deploy function -f profiles",
    "deploy:api": "serverless deploy function -f api",
    "lint": "eslint ./src",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "commit": "npm run lint && npm run test"
  },
  "author": "Evan Fleming",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-sanitizer": "^1.0.5",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11",
    "serverless-http": "^2.3.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "serverless-offline": "^5.12.1"
  }
}
